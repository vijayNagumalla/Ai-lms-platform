{
  "name": "lms-backend",
  "version": "1.0.0",
  "description": "LMS Platform Backend API",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "migrate": "node scripts/runMigrations.js",
    "migrate:status": "node -e \"import('./utils/migrationRunner.js').then(m => m.default.getMigrationStatus().then(s => console.table(s)))\""
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "formidable": "^3.5.4",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "judge0-client": "^0.1.0",
    "multer": "^2.0.2",
    "@supabase/supabase-js": "^2.39.0",
    "mysql2": "^3.15.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.10",
    "pdfkit": "^0.17.2",
    "prop-types": "^15.8.1",
    "puppeteer": "^24.31.0",
    "react-hot-toast": "^2.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "lms",
    "education",
    "api",
    "mysql"
  ],
